{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "require-jsdoc": "off",
    "indent": "off",
    "comma-dangle": "off",
    "no-unused-vars": "off",
    "react/react-in-jsx-scope": "off",
    "max-len": "off",
    "operator-linebreak": "off",
    "linebreak-style": "off"
  }
}
