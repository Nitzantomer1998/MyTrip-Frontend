{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off",
    "padded-blocks": "off",
    "react/react-in-jsx-scope": "off",
    "max-len": ["warn", { "code": 500 }],
    "no-unused-vars": "off"
  }
}
