{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": "off",
    "arrow-parens": "off",
    "padded-blocks": "off",
    "indent": "off",
    "react/react-in-jsx-scope": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "max-len": 500
  }
}
